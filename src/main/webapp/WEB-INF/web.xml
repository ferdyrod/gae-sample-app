<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>AuthServlet</servlet-name>
        <servlet-class>AuthServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAutomovilServlet</servlet-name>
        <servlet-class>AddAutomovilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMotocicletaServlet</servlet-name>
        <servlet-class>AddMotocicletaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBicicletaServlet</servlet-name>
        <servlet-class>AddBicicletaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthServlet</servlet-name>
        <url-pattern>/AuthServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAutomovilServlet</servlet-name>
        <url-pattern>/add_automovil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMotocicletaServlet</servlet-name>
        <url-pattern>/add_motocicleta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBicicletaServlet</servlet-name>
        <url-pattern>/add_bicicleta</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>

    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <listener>
        <listener-class>OfyHelper</listener-class>
    </listener>
</web-app>
